= MassiveClient(apiKey: apiKey)","","\/\/ Fetch dividend history","let dividends = try await client.dividends(DividendsQuery(ticker: \"AAPL\"))","for dividend in dividends.results ?? [] {","    print(\"Ex-Date: \\(dividend.exDividendDate ?? \"\") - $\\(dividend.cashAmount ?? 0)\")","}","","\/\/ Fetch stock splits","let splits = try await client.splits(SplitsQuery(ticker: \"AAPL\"))","for split in splits.results ?? [] {","    print(\"\\(split.executionDate ?? \"\"): \\(split.splitFrom ?? 0)-for-\\(split.splitTo ?? 0)\")","}","","\/\/ Fetch upcoming IPOs","let ipos = try await client.ipos(IPOsQuery(ipoStatus: .pending))","for ipo in ipos.results ?? [] {","    print(\"\\(ipo.ticker ?? \"\"): \\(ipo.issuerName ?? \"\") listing \\(ipo.listingDate ?? \"\")\")","}"],"syntax":"swift"},{"type":"heading","level":3,"anchor":"IPO-Status-Options","text":"IPO Status Options"},{"inlineContent":[{"type":"text","text":"Filter upcoming IPOs by status:"}],"type":"paragraph"},{"type":"table","header":"row","rows":[[[{"inlineContent":[{"type":"text","text":"Status"}],"type":"paragraph"}],[{"type":"paragraph","inlineContent":[{"type":"text","text":"Description"}]}]],[[{"inlineContent":[{"type":"codeVoice","code":".pending"}],"type":"paragraph"}],[{"inlineContent":[{"text":"IPO is scheduled but not yet priced","type":"text"}],"type":"paragraph"}]],[[{"inlineContent":[{"code":".priced","type":"codeVoice"}],"type":"paragraph"}],[{"inlineContent":[{"type":"text","text":"IPO has been priced"}],"type":"paragraph"}]],[[{"type":"paragraph","inlineContent":[{"type":"codeVoice","code":".withdrawn"}]}],[{"inlineContent":[{"type":"text","text":"IPO was cancelled"}],"type":"paragraph"}]]]},{"anchor":"Economic-Indicators","type":"heading","text":"Economic Indicators","level":2},{"inlineContent":[{"type":"text","text":"Access macroeconomic data from Federal Reserve sources:"}],"type":"paragraph"},{"code":["let client = MassiveClient(apiKey: apiKey)","","\/\/ Fetch Treasury yield curve","let yields = tr