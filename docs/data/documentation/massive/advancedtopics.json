de":["let client = MassiveClient(apiKey: apiKey)","","\/\/ Fetch dividend history","let dividends = try await client.dividends(DividendsQuery(ticker: \"AAPL\"))","for dividend in dividends.results ?? [] {","    print(\"Ex-Date: \\(dividend.exDividendDate ?? \"\") - $\\(dividend.cashAmount ?? 0)\")","}","","\/\/ Fetch stock splits","let splits = try await client.splits(SplitsQuery(ticker: \"AAPL\"))","for split in splits.results ?? [] {","    print(\"\\(split.executionDate ?? \"\"): \\(split.splitFrom ?? 0)-for-\\(split.splitTo ?? 0)\")","}","","\/\/ Fetch upcoming IPOs","let ipos = try await client.ipos(IPOsQuery(ipoStatus: .pending))","for ipo in ipos.results ?? [] {","    print(\"\\(ipo.ticker ?? \"\"): \\(ipo.issuerName ?? \"\") listing \\(ipo.listingDate ?? \"\")\")","}"]},{"text":"IPO Status Options","anchor":"IPO-Status-Options","type":"heading","level":3},{"inlineContent":[{"type":"text","text":"Filter upcoming IPOs by status:"}],"type":"paragraph"},{"rows":[[[{"type":"paragraph","inlineContent":[{"type":"text","text":"Status"}]}],[{"type":"paragraph","inlineContent":[{"type":"text","text":"Description"}]}]],[[{"type":"paragraph","inlineContent":[{"type":"codeVoice","code":".pending"}]}],[{"type":"paragraph","inlineContent":[{"type":"text","text":"IPO is scheduled but not yet priced"}]}]],[[{"type":"paragraph","inlineContent":[{"type":"codeVoice","code":".priced"}]}],[{"type":"paragraph","inlineContent":[{"type":"text","text":"IPO has been priced"}]}]],[[{"type":"paragraph","inlineContent":[{"type":"codeVoice","code":".withdrawn"}]}],[{"type":"paragraph","inlineContent":[{"type":"text","text":"IPO was cancelled"}]}]]],"header":"row","type":"table"},{"text":"Economic Indicators","anchor":"Economic-Indicators","type":"heading","level":2},{"inlineContent":[{"type":"text","text":"Access macroeconomic data from Federal Reserve sources:"}],"type":"paragraph"},{"syntax":"swift","type":"codeListing","code":["let client = MassiveClient(apiKey: apiKey)","","\/\/ Fetch Treasury yield curve","let yields = try await client.treasuryYields(TreasuryYieldsQuery())","for yield in yields.results ?? [] {","    print(\"\\(yield.date ?? \"\"): 2Y=\\(yield.yield2Year ?? 0)% 10Y=\\(yield.yield10Year ?? 0)%\")","}","","\/\/ Fetch inflation data (CPI, PCE)","let inflation = try await client.inflation(InflationQuery())","for obs in inflation.results ?? [] {","    print(\"\\(obs.date ?? \"\"): CPI YoY=\\(obs.cpiYearOverYear ?? 0)%\")","}","","\/\/ Fetch labor market data","let labor = try await client.laborMarket(LaborMarketQuery())","for obs in labor.results ?? [] {","    print(\"\\(obs.date ?? \"\"): Unemployment=\\(obs.unemploymentRate ?? 0)%\")","}"]},{"text":"Available Indicators","anchor":"Available-Indicators","type":"heading","level":3},{"rows":[[[{"type":"paragraph","inlineContent":[{"type":"text","text":"Endpoint"}]}],[{"type":"paragraph","inlineContent":[{"type":"text","text":"Data"}]}]],[[{"type":"paragraph","inlineContent":[{"type":"codeVoice","code":"treasuryYields"}]}],[{"type":"paragraph","inlineContent":[{"type":"text","text":"Yield curve from 1M to 30Y"}]}]],[[{"type":"paragraph","inlineContent":[{"type":"codeVoice","code":"inflation"}]}],[{"type":"paragraph","inlineContent":[{"type":"text","text":"CPI, PCE, Core inflation"}]}]],[[{"type":"paragraph","inlineContent":[{"type":"codeVoice","code":"inflationExpectations"}]}],[{"type":"paragraph","inlineContent":[{"type":"text","text":"Breakeven rates, model forecasts"}]}]],[[{"type":"paragraph","inlineContent":[{"code":"laborMarket","type":"codeVoice"}]}],[{"type":"paragraph","inlineContent":[{"text":"Unemployment, job openings, wages","type":"text"}]}]]],"header":"row","type":"table"},{"text":"Market Snapshots","anchor":"Market-Snapshots","type":"heading","level":2},{"inlineContent":[{"text":"Get real-time market data and top movers:","type":"text"}],"type":"paragraph"},{"syntax":"swift","type":"codeListing","code":["let client = MassiveClient(apiKey: apiKey)","","\/\/ Get real-time