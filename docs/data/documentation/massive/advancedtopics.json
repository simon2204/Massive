apshot = try await client.tickerSnapshot(SingleTickerSnapshotQuery(ticker: \"AAPL\"))","if let ticker = snapshot.ticker {","    print(\"Last Price: $\\(ticker.lastTrade?.p ?? 0)\")","    print(\"Today's Change: \\(ticker.todaysChangePerc ?? 0)%\")","}","","\/\/ Get top market gainers","let gainers = try await client.topMovers(TopMoversQuery(direction: .gainers))","print(\"Top Gainers:\")","for ticker in gainers.tickers ?? [] {","    print(\"  \\(ticker.ticker ?? \"\"): +\\(ticker.todaysChangePerc ?? 0)%\")","}","","\/\/ Get top market losers","let losers = try await client.topMovers(TopMoversQuery(direction: .losers))","print(\"Top Losers:\")","for ticker in losers.tickers ?? [] {","    print(\"  \\(ticker.ticker ?? \"\"): \\(ticker.todaysChangePerc ?? 0)%\")","}"],"type":"codeListing","syntax":"swift"},{"text":"Snapshot Types","anchor":"Snapshot-Types","type":"heading","level":3},{"type":"table","rows":[[[{"inlineContent":[{"text":"Method","type":"text"}],"type":"paragraph"}],[{"inlineContent":[{"type":"text","text":"Description"}],"type":"paragraph"}]],[[{"type":"paragraph","inlineContent":[{"type":"codeVoice","code":"tickerSnapshot"}]}],[{"type":"paragraph","inlineContent":[{"text":"Single ticker with full details","type":"text"}]}]],[[{"inlineContent":[{"type":"codeVoice","code":"marketSnapshot"}],"type":"paragraph"}],[{"type":"paragraph","inlineContent":[{"type":"text","text":"All tickers or a filtered list"}]}]],[[{"inlineContent":[{"type":"codeVoice","code":"universalSnapshot"}],"type":"paragraph"}],[{"inlineContent":[{"type":"text","text":"Mixed asset types in one request"}],"type":"paragraph"}]],[[{"inlineContent":[{"type":"codeVoice","code":"topMovers"}],"type":"paragraph"}],[{"inlineContent":[{"text":"Top 20 gainers or losers","type":"text"}],"type":"paragraph"}]]],"header":"row"}]}],"schemaVersion":{"major":0,"minor":3,"patch":0},"references":{"doc://Massive/documentation/Massive/FlatFiles":{"identifier":"doc:\/\/Massive\/documentation\/Massive\/FlatFile