{"hierarchy":{"paths":[["doc:\/\/Massive\/documentation\/Massive"]]},"schemaVersion":{"major":0,"minor":3,"patch":0},"identifier":{"interfaceLanguage":"swift","url":"doc:\/\/Massive\/documentation\/Massive\/FlatFiles"},"kind":"article","primaryContentSections":[{"kind":"content","content":[{"level":2,"type":"heading","text":"Overview","anchor":"Overview"},{"inlineContent":[{"type":"text","text":"Massive Flat Files provide compressed CSV files containing historical market data. This is more efficient than making individual REST API requests when you need large datasets."}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"Data is organized by:"}],"type":"paragraph"},{"items":[{"content":[{"type":"paragraph","inlineContent":[{"inlineContent":[{"type":"text","text":"Asset Class"}],"type":"strong"},{"type":"text","text":": "},{"isActive":true,"type":"reference","identifier":"doc:\/\/Massive\/documentation\/Massive\/AssetClass\/usStocks"},{"type":"text","text":", "},{"isActive":true,"type":"reference","identifier":"doc:\/\/Massive\/documentation\/Massive\/AssetClass\/usOptions"},{"type":"text","text":", "},{"isActive":true,"type":"reference","identifier":"doc:\/\/Massive\/documentation\/Massive\/AssetClass\/indices"},{"type":"text","text":", "},{"isActive":true,"type":"reference","identifier":"doc:\/\/Massive\/documentation\/Massive\/AssetClass\/forex"},{"type":"text","text":", "},{"isActive":true,"type":"reference","identifier":"doc:\/\/Massive\/documentation\/Massive\/AssetClass\/crypto"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"inlineContent":[{"type":"text","text":"Data Type"}],"type":"strong"},{"type":"text","text":": "},{"isActive":true,"type":"reference","identifier":"doc:\/\/Massive\/documentation\/Massive\/DataType\/trades"},{"type":"text","text":", "},{"isActive":true,"type":"reference","identifier":"doc:\/\/Massive\/documentation\/Massive\/DataType\/quotes"},{"type":"text","text":", "},{"isActive":true,"type":"reference","identifier":"